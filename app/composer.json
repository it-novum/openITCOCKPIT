{
    "name": "it-novum/openITCOCKPIT",
    "description": "openITCOCKPIT is an open source monitoring framework based on nagios",
    "version": "3.0.7",
    "homepage": "http://openitcockpit.org",
    "license": "Dual licensed GPLv3 and proprietary",
    "authors": [
        {
            "name": "it-novum GmbH",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "itnovum\\openITCOCKPIT\\": "src/itnovum/openITCOCKPIT/",
            "Statusengine\\": "src/Statusengine/",
            "Adldap\\": "Vendor/adldap/adldap/src/",
            "App\\": "cake4/src/",
            "MkModule\\": "cake4/plugins/MkModule/src/",
            "Statusengine2Module\\": "cake4/plugins/Statusengine2Module/src/",
            "DistributeModule\\": "cake4/plugins/DistributeModule/src/"
        },
        "files": [
            "Vendor/imageSmoothArc.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "cake4/tests/"
        },
        "files": [
            "Vendor/imageSmoothArc.php"
        ]
    },
    "config": {
        "vendor-dir": "Vendor"
    },
    "scripts": {
        "cs-check": "Vendor/bin/phpcs --colors -p --standard=cake4/analysis/codestyle_openitcockpit.xml ./cake4/src ./cake4/tests",
        "cs-check-strict": "Vendor/bin/phpcs -pn --ignore=./cake4/config/Migrations/*,./cake4/config/Seeds/*,./cake4/config/schema.php  --standard=cake4/analysis/openitcockpit-strict.xml ./cake4/src ./cake4/tests ./cake4/plugins ./cake4/config",
        "cs-fix": "Vendor/bin/phpcbf --colors --standard=cake4/analysis/codestyle_openitcockpit.xml ./cake4/src ./cake4/tests",
        "stan": "php -d memory_limit=-1 Vendor/bin/phpstan analyse -c cake4/analysis/phpstan.neon -l 1 ./cake4/src ./cake4/plugins",
        "php-syntax": "cake4/analysis/php_syntax.sh cake4/src/ cake4/tests/ cake4/plugins/",
        "test": "Vendor/bin/phpunit --colors=always --configuration=cake4/analysis/phpunit.xml",
        "coverage": "Vendor/bin/phpunit --configuration=cake4/analysis/phpunit.xml --coverage-html cake4/tmp/phpunit/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "symfony/filesystem": "^3.2",
        "symfony/finder": "^3.2",
        "crate/crate-pdo": "~0.6.0",
        "crate/crate-dbal": "~0.3.0",
        "ddeboer/imap": "^0.5.2",
        "smottt/wideimage": "^1.1",
        "erusev/parsedown": "^1.6",
        "erusev/parsedown-extra": "^0.7.1",
        "cboden/ratchet": "^0.4.0",
        "matthiasmullie/minify": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^3.4",
        "guiguiboy/php-cli-progress-bar": "^0.0.4",
        "cakephp/cakephp": "4.x-dev",
        "phpstan/phpstan": "^0.9.2",
        "jbbcode/jbbcode": "^1.3",
        "freedsx/ldap": "^0.4.0",
        "symfony/yaml": "^3.4",
        "azuyalabs/yasumi": "^2.1",
        "cakephp/migrations": "4.x-dev"
    },
    "require-dev": {
        "cakephp/bake": "4.x-dev",
        "slevomat/coding-standard": "3.0",
        "phpunit/phpunit": "6.5.13"
    }
}
